<div class="modal" id="datosContacto" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" translate="INFORMACION_CONTACTO"></h4>
            </div>
            <div class="modal-body">
                <img class="foto-contacto" height="170px" width="170px" ngf-src="contactoSeleccionado.foto"
                     alt="Foto contacto"/>

                <div class="datos-contacto">

                    <h4 class="titulo" translate="USERNAME"></h4>
                    <span>{{contactoSeleccionado.username}}</span>

                    <h4 class="titulo" translate="NOMBRE"></h4>
                    <span>{{contactoSeleccionado.nombre}}</span>

                    <h4 class="titulo" translate="APELLIDOS"></h4>
                    <span>{{contactoSeleccionado.apellidos}}</span>

                    <h4 class="titulo" translate="EMAIL"></h4>
                    <span>{{contactoSeleccionado.email}}</span>

                    <h4 class="titulo" translate="ACCIONES"></h4>
                    <button class="btn btn-danger" translate="BLOQUEAR_CONTACTO"
                            ng-click="bloquearContacto(contactoSeleccionado.username)"></button>

                </div>

            </div>

            <div class="modal-footer">
                <button class="btn btn-primary" data-dismiss="modal">{{'CERRAR' | translate}}</button>
            </div>

        </div>
    </div>
</div>