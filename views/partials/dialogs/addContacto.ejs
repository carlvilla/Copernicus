<div ng-controller="addContactoController" class="modal" id="addUsuario" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">{{'ADD_USUARIO' | translate}}</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">

                    <label for="username-solicitud-contacto">{{'USERNAME' | translate}}</label>

                    <angucomplete id="username-solicitud-contacto"
                                  placeholder="{{'USERNAME' | translate}}"
                                  pause="100"
                                  selectedobject="usuarioSeleccionado"
                                  localdata="usuarios"
                                  titlefield="username"
                                  searchfields="username"
                                  imagefield="foto"
                                  minlength="2"
                                  inputclass="form-control form-control-small"
                    />


                </div>

                <textarea id="mensaje-solicitud-contacto" ng-model="mensaje"
                          placeholder="{{'MENSAJE_ENVIADO_SOLICITUD' | translate}}"></textarea>

            </div>
            <div class="modal-footer">
                <button ng-disabled="!usuarioSeleccionado" data-dismiss="modal" class="btn btn-primary"
                        ng-click="enviarSolicitudContacto()">{{'MANDAR_SOLICITUD_CONTACTO' | translate}}
                </button>
                <button class="btn btn-primary" data-dismiss="modal">{{'CERRAR' | translate}}</button>
            </div>
        </div>
    </div>
</div>